<templateSet group="User-Java">
  <template name="ttest" value="@Test&#10;public void $NAME$() {&#10;    // given&#10;    $END$&#10;    &#10;    // when&#10;    &#10;    &#10;    // then&#10;    &#10;}" description="TestNG Test method" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="tbeforem" value="@BeforeMethod&#10;public void beforeMethod() {&#10;    $END$&#10;}" description="TestNG before Test" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="logger" value="private static final Logger LOG = LoggerFactory.getLogger($CLASS_NAME$.class);" description="Logger" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>